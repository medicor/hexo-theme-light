#header
  padding 60px 0 20px 0
  a
    color #999
    &:hover
      color color-link
      text-decoration none
    
  @media screen and (max-width: 600px)
    margin 10px 10px

  h1
    text-shadow 1px 1px 3px #fff
    font-weight normal
    font-size 2.8em
    margin 0

  h2
    font-weight normal
    font-size 1.0em
    margin 0
